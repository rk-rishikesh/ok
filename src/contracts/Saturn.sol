contract Saturn is KeeperCompatibleInterface {
    
    uint256 public nominationIds;

    struct Nomination {
        uint256 nominationId;
        IERC721 tokenAddress;
        uint256 tokenID;
        address nominatedBy;
        address nominee;
        bool fetchRequestInitiated;
        uint256 fetchRequestInitiatedAt;
        bool fetched;
        bool valid;
    }

    uint public immutable interval;
    uint public lastTimeStamp;

    mapping(uint256 => Nomination) public nominations;

    uint public counter;

    constructor(uint updateInterval) {
      interval = updateInterval;
      lastTimeStamp = block.timestamp;
      counter = 0;
    }

    function checkUpkeep(bytes calldata /* checkData */) external view override returns (bool upkeepNeeded, bytes memory /* performData */) {
        upkeepNeeded = (block.timestamp - lastTimeStamp) > interval;
        // We don't use the checkData in this example. The checkData is defined when the Upkeep was registered.
    }

    function booola(uint256 i) public view returns(bool) {
        return IERC721(nominations[i].tokenAddress).ownerOf(nominations[i].tokenID) != nominations[i].nominatedBy;
    }

    function performUpkeep(bytes calldata /*performData*/) external override {
        
        for(uint i = 1; i <= nominationIds; i++) {
            if(IERC721(nominations[i].tokenAddress).ownerOf(nominations[i].tokenID) != nominations[i].nominatedBy) {
                nominations[i].valid = false;

                lastTimeStamp = block.timestamp;
                counter = counter + i;
            }
        }
    
    }

    // 1. Nominate
    function Nominate(address _tokenAddress, uint256 _tokenId, address _nominee) public {
        require(IERC721(_tokenAddress).ownerOf(_tokenId) == msg.sender, "You are not the owner of this NFT");
        require(
            IERC721(_tokenAddress).isApprovedForAll(IERC721(_tokenAddress).ownerOf(_tokenId), address(this)),
            "NFT not approved for transfer"
        );

        nominationIds = nominationIds + 1;

        Nomination memory newNomination = Nomination({
            nominationId: nominationIds,
            tokenAddress:  IERC721(_tokenAddress),
            tokenID: _tokenId,
            nominatedBy: msg.sender,
            nominee: _nominee,
            fetchRequestInitiated: false,
            fetchRequestInitiatedAt: 0,
            fetched: false,
            valid: true
        });
 
        nominations[nominationIds] = newNomination;

    }

    // 2. Initiate fetch Request
    function initiateFetchRequest(uint256 _nominationId) public {
        require(nominations[_nominationId].nominee == msg.sender, "Only nominee can initiate the fetch request");
        nominations[_nominationId].fetchRequestInitiated =  true;
        nominations[_nominationId].fetchRequestInitiatedAt = block.timestamp;
    }

    // 3. Reject fetch Request
    function rejectFetchRequest(uint256 _nominationId) public {
        require(nominations[_nominationId].nominatedBy == msg.sender, "Only token owner can reject the fetch request");
        require(IERC721(nominations[_nominationId].tokenAddress).ownerOf(nominations[_nominationId].tokenID) == msg.sender, "You are not owner of NFT");
        require(nominations[_nominationId].fetchRequestInitiatedAt + 1 days > block.timestamp, "Fetch period finished");
        nominations[_nominationId].fetchRequestInitiated =  false;
        nominations[_nominationId].fetchRequestInitiatedAt = 0;
    }

    // 4. ExecuteTransfer
    function executeTransfer(uint256 _nominationId) public {
        require(IERC721(nominations[_nominationId].tokenAddress).ownerOf(nominations[_nominationId].tokenID) == nominations[_nominationId].nominatedBy, "The nominator is not the current owner");
        require(nominations[_nominationId].nominee == msg.sender, "Only nominee");
        // this would be done after the fetch time period gets over
        require(nominations[_nominationId].fetchRequestInitiated, "Not fetched");
        require(nominations[_nominationId].fetchRequestInitiatedAt + 1 days < block.timestamp, "Fetch period not finished yet");
        IERC721(nominations[_nominationId].tokenAddress).safeTransferFrom(nominations[_nominationId].nominatedBy, msg.sender, nominations[_nominationId].tokenID);
    }
}